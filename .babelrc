{
  "env": {
    "production": {
      "presets": ["minify"]
    }
  },
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-pipeline-operator",
    "@babel/plugin-transform-flow-strip-types",
    "@babel/plugin-syntax-dynamic-import",
    "react-hot-loader/babel"
  ]
}